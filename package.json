{
  "name": "nano-banana-mcp",
  "version": "2.0.0",
  "description": "MCP server for AI image generation and editing using Google Gemini (Flash + Pro models)",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "nano-banana-mcp": "./dist/index.js"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run typecheck"
  },
  "keywords": [
    "mcp",
    "nano-banana",
    "gemini",
    "image-generation",
    "ai",
    "google-ai",
    "claude-code",
    "cursor",
    "image-editing"
  ],
  "author": "fjacquet (forked from ConechoAI)",
  "license": "MIT",
  "homepage": "https://github.com/fjacquet/Nano-Banana-MCP#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fjacquet/Nano-Banana-MCP.git"
  },
  "bugs": {
    "url": "https://github.com/fjacquet/Nano-Banana-MCP/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@google/genai": "^1.39.0",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "dotenv": "^16.4.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
